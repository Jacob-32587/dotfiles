    "latex-workshop.latex.external.build.args": [
        "--shell-escape"
    ],
    "workbench.editor.tabSizing": "shrink",
    "editor.tabSize": 4,
    "editor.insertSpaces": true,
    "editor.detectIndentation": true,
    "security.workspace.trust.untrustedFiles": "open",
    "editor.rulers": [
        {
            "color": "#ffffff22",
            "column": 110
        }
    ],
    "explorer.confirmDelete": false,
    "editor.inlayHints.enabled": "offUnlessPressed",
    "editor.fontFamily": "GohuFont 11 Nerd Font Mono",
    "rust-analyzer.check.command": "clippy",
    "crates.compatibleDecorator": "",
    "crates.incompatibleDecorator": " ${version}",
    "crates.errorDecorator": "",
    "crates.incompatibleDecoratorCss": {
        "after": {
            "color": "#d5cc29c1",
            // "backgroundColor": "#783a3a92"
        }
    },
    "crates.compatibleDecoratorCss": {
        "after": {
            "color": "#3ab53aee",
            // "border": "2px dashed blue"
        }
    },
    "crates.errorDecoratorCss": {
        "after": {
            "color": "#d43b3bee",
            // "border": "2px dashed blue"
        }
    },
    "terminal.integrated.automationProfile.linux": {
        "path": "/bin/bash",
        "args": ["-e"],
        "icon": "symbol-null"
    },
    "terminal.integrated.fontFamily": "monospace",
